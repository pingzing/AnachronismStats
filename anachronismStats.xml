    <!-- A template for a section header -->
    <Frame name="AS_HeaderFrameTemplate" virtual="true">
        <Size x="115" y="13"/>        
        <Layers>
            <Layer level="BACKGROUND">
                <FontString name="$parentHeader" parentKey="Header" inherits="GameFontNormalSmall">
                    <Anchors>
                        <Anchor point="LEFT"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>

    <!-- A template for a line of stats that has a name and a value -->
    <Frame name="AS_StatFrameTemplate" virtual="true">
        <Size x="115" y="13"/>        
        <Layers>
            <Layer level="BACKGROUND">
                <FontString name="$parentStatLabel" parentKey="Label" inherits="GameFontNormalSmallWhite">
                    <Anchors>
                        <Anchor point="LEFT"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Frame name="$parentStat" parentKey="ValueFrame">
                <Size x="30" y="13"/> 
                <Anchors>
                    <Anchor point="RIGHT"/>
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <FontString name="$parentText" parentKey="Value" inherits="GameFontNormalSmallWhite" justifyH="RIGHT">
                            <Anchors>
                                <Anchor point="RIGHT"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>
        </Frames>
        <Scripts>
            <OnEnter>
                AS_ShowStatTooltip(self);
            </OnEnter>
            <OnLeave>
                GameTooltip:Hide();
            </OnLeave>
        </Scripts>
    </Frame>
    
    <!-- Button to show/hide AS Stats panel -->
    <Button name="AS_OpenStats" inherits="UIPanelSquareButton" motionScriptsWhileDisabled="false" parent="PaperDollFrame">
        <Anchors>
            <Anchor point="TOPLEFT" relativeTo="PaperDollFrame" relativePoint="TOPLEFT">
                <Offset>
                    <AbsDimension x="306" y="-40"/>
                </Offset>
            </Anchor>
        </Anchors>
        <Scripts>
            <OnClick function="AS_OnClick"/>
            <OnEnter>
                GameTooltip:SetOwner(self, "ANCHOR_RIGHT");
                GameTooltip:SetText(MicroButtonTooltipText("Anachronism Stats", ""), 1.0,1.0,1.0 );
            </OnEnter>
            <OnClick function="AS_OpenStats_OnClick"/>
            <OnLeave function="GameTooltip_Hide"/>
            <OnLoad function="AS_OpenStats_OnLoad"/>
            <OnHide function="AS_OpenStats_OnHide"/>
        </Scripts>
    </Button>
    
